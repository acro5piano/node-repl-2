{
  "name": "node-repl-2",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:acro5piano/node-repl-2",
  "author": "gosho-kazuya <ketsume0211@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "env NODE_PATH=./src ./node_modules/.bin/tsc",
    "test": "jest --maxWorkers=10",
    "watch:ts": "tsc -w",
    "watch:js": "env NODE_PATH=./dist nodemon --delay 2 --watch dist dist/index.js",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "strip-ansi": "^4.0.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.2",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.6",
    "@types/strip-ansi": "^3.0.0",
    "@types/webpack-env": "^1.13.6",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^7.1.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.1"
  }
}
